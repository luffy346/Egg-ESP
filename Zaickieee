-- Zaickie Grow A Garden ESP with Draggable Logo & Toggle
-- For Delta Executor

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

-- SETTINGS
local ESP_COLOR = Color3.fromRGB(255, 0, 0) -- Red
local TITLE = "Zaickie"
local LOGO_ID = "rbxassetid://1234567890" -- Replace with your image ID

-- UI BASE
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ZaickieESP"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

-- LOGO BUTTON (Draggable)
local LogoFrame = Instance.new("Frame", ScreenGui)
LogoFrame.Size = UDim2.new(0, 120, 0, 140)
LogoFrame.Position = UDim2.new(0, 20, 0, 200)
LogoFrame.BackgroundTransparency = 1

local LogoImage = Instance.new("ImageButton", LogoFrame)
LogoImage.Size = UDim2.new(0, 100, 0, 100)
LogoImage.Position = UDim2.new(0.5, -50, 0, 0)
LogoImage.BackgroundTransparency = 1
LogoImage.Image = LOGO_ID

local LogoText = Instance.new("TextLabel", LogoFrame)
LogoText.Size = UDim2.new(1, 0, 0, 30)
LogoText.Position = UDim2.new(0, 0, 1, -30)
LogoText.Text = TITLE
LogoText.TextColor3 = ESP_COLOR
LogoText.BackgroundTransparency = 1
LogoText.TextScaled = true
LogoText.TextStrokeTransparency = 0

-- DRAG FUNCTION
local dragging = false
local dragInput, dragStart, startPos

LogoFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = LogoFrame.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

LogoFrame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		LogoFrame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
end)

-- TOGGLE BOX (Hidden by default)
local ToggleBox = Instance.new("Frame", LogoFrame)
ToggleBox.Size = UDim2.new(0, 150, 0, 40)
ToggleBox.Position = UDim2.new(0.5, -75, 1, 10)
ToggleBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ToggleBox.Visible = false

local ToggleLabel = Instance.new("TextLabel", ToggleBox)
ToggleLabel.Size = UDim2.new(0.7, 0, 1, 0)
ToggleLabel.Text = "Egg ESP"
ToggleLabel.TextColor3 = ESP_COLOR
ToggleLabel.BackgroundTransparency = 1
ToggleLabel.TextScaled = true

local ToggleButton = Instance.new("TextButton", ToggleBox)
ToggleButton.Size = UDim2.new(0.3, 0, 1, 0)
ToggleButton.Position = UDim2.new(0.7, 0, 0, 0)
ToggleButton.Text = "OFF"
ToggleButton.TextColor3 = Color3.new(1, 0, 0)
ToggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
ToggleButton.TextScaled = true

-- CLICK LOGO TO SHOW/HIDE TOGGLE BOX
LogoImage.MouseButton1Click:Connect(function()
	ToggleBox.Visible = not ToggleBox.Visible
end)

-- EGG ESP FUNCTION
local EggESPEnabled = false

local function createESP(part)
	if part:FindFirstChild("ZaickieESP") then return end
	local billboard = Instance.new("BillboardGui")
	billboard.Name = "ZaickieESP"
	billboard.Adornee = part
	billboard.Size = UDim2.new(0, 100, 0, 40)
	billboard.AlwaysOnTop = true

	local textLabel = Instance.new("TextLabel", billboard)
	textLabel.Size = UDim2.new(1, 0, 1, 0)
	textLabel.Text = "ðŸ¥š EGG"
	textLabel.TextColor3 = ESP_COLOR
	textLabel.BackgroundTransparency = 1
	textLabel.TextStrokeTransparency = 0
	textLabel.TextScaled = true

	billboard.Parent = part
end

RunService.RenderStepped:Connect(function()
	if EggESPEnabled then
		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("BasePart") and obj.Name:lower():find("egg") then
				createESP(obj)
			end
		end
	else
		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("BasePart") then
				local esp = obj:FindFirstChild("ZaickieESP")
				if esp then
					esp:Destroy()
				end
			end
		end
	end
end)

ToggleButton.MouseButton1Click:Connect(function()
	EggESPEnabled = not EggESPEnabled
	if EggESPEnabled then
		ToggleButton.Text = "ON"
		ToggleButton.TextColor3 = Color3.fromRGB(0, 255, 0)
	else
		ToggleButton.Text = "OFF"
		ToggleButton.TextColor3 = Color3.fromRGB(255, 0, 0)
	end
end)

print(TITLE .. " ESP Loaded âœ…")
